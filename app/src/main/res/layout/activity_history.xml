<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical"
    tools:context="com.mxkj.econtrol.ui.activity.SettingActivity">


    <include layout="@layout/incloud_title_layout2"></include>

    <RelativeLayout
        android:id="@+id/rl_controllog"
        style="@style/FragmentMy_RelativeLayout_Prass"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:text="操作记录"
            android:textColor="@color/text_black_666"
            android:textSize="32px"
            android:drawableLeft="@drawable/ic_my_personal_history"
            android:drawablePadding="24px"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/center_enter" />



    </RelativeLayout>
    <View
        style="@style/view_line"
        android:layout_alignParentBottom="true" />

    <RelativeLayout
        android:id="@+id/rl_use_num"
        style="@style/FragmentMy_RelativeLayout_Prass"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:text="使用次数"
            android:textColor="@color/text_black_666"
            android:textSize="32px"
            android:drawableLeft="@drawable/ic_history_control_xiguan"
            android:drawablePadding="24px"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/center_enter" />



    </RelativeLayout>
    <View
        style="@style/view_line"
        android:layout_alignParentBottom="true" />

    <RelativeLayout
        android:id="@+id/rl_use_time"
        style="@style/FragmentMy_RelativeLayout_Prass"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center"
            android:text="使用时长"
            android:textColor="@color/text_black_666"
            android:textSize="32px"
            android:drawableLeft="@drawable/ic_usage_time"
            android:drawablePadding="24px"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/center_enter" />


    </RelativeLayout>

    <View style="@style/view_line" />



</LinearLayout>
